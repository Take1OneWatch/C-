#include<stdio.h>
void main()
{
    int k,l;
	printf("下面请看，这是原本的是52张牌\n");
	for(l=1;l<=52;l++)
	{
		printf("%d\t",l);
	}
    printf("\n输入数字1即可得到52次翻牌的结果：");
	scanf("%d",&k);
	if(k==1)
	{
			int i,j,s[52];
	for(i=2;i<=52;i++)
		for(j=i-1;j<52;j+=i)
			s[j]=!s[j];
		printf("正面向上的牌为\n");
		for(i=0;i<52;i++)
			if(s[i])
				printf("%4d",i+1);

	}

}
