#include<stdio.h>
void main()
{
	//本题中正整数代表牌面向上，0表示拍背向上
    int i,j,n=0;	
	for(i=1;i<=52;i++)
	{  
		if(i%2==0||i%3==0||i%4==0)
		{
			if(i%6==0)
			{
               printf("%d\t",i);
			   continue;
			}
			if(i%8==0||i%12==0||i%24==0)
			{
				 printf("%d\t",i);
			   continue;
			}
		  printf("%d\t",n); 
		}
       else
	   {
		   printf("%d\t",i);
	   } 
	}		
}
