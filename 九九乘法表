#include <stdio.h>
void main()
{
	int i = 1 ;int j = 1;
		for(i=9;i>=1;i--)
		{
		for(j=1;j<=i;j++)
		printf("%d*%d=%-4d",i,j,i*j);
	printf("\n");
		}
}
//倒九九乘法表！




#include<stdio.h>
void main()
{
	int s[10],i,max,min,j,k;
	for(i=0;i<10;i++)
	{scanf("%d",&s[i]);}
	max=min=s[0];
    j=k=0;
	for(i=0;i<10;i++)
	{if(max<s[i]){max=s[i];j=i;}
	 if(min>s[i]){min=s[i];k=i;}
	}
	printf("max:s[%d]=%d,min:s[%d]=%d",j,max,k,min);
    printf("\n");
}
//求最大最小值

