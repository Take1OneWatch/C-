#include<stdio.h>
#include<stdlib.h>
int main()
{
int a[1000];
int m;
int n;
int x=0;
int count;//定义一个count，记录圆圈中剩余的猴子数
int i;
printf("请说出你要生几个猴子\n");
 scanf("%d",&m);
 printf("请说出你要几个几个轮一次\n");
 scanf("%d",&n);
 count=m;//加这句
 for(i=0;count>1;i++)//结束条件是猴子总数只剩下一个
 	{
     if(a[i%m]!=-1)
     	{
  		x+=1;
  		}
  	if(x%n==0&&a[i%m]!=-1)
  		{
  		a[i%m]=-1;
  		count--;
  		}
 	}
 for(i=0;i<m;i++)
   if(a[i]!=-1)	
     printf("%d\n",i+1);
return 0;
}
